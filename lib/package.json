{
  "name": "ngrx-data",
  "version": "6.1.0-beta.3",
  "repository": "https://github.com/johnpapa/angular-ngrx-data.git",
  "license": "MIT",
  "peerDependencies": {
    "@angular/core": ">=6.1.0 <8.0.0",
    "@angular/common": ">=6.1.0 <8.0.0",
    "@ngrx/effects": "^6.0.0",
    "@ngrx/entity": "^6.0.0",
    "@ngrx/store": "^6.0.0",
    "rxjs": "^6.1.0",
    "tslib": "^1.9.0"
  },
  "ngPackage": {
    "$schema": "../node_modules/ng-packagr/ng-package.schema.json",
    "dest": "../dist/ngrx-data",
    "workingDirectory": "../.ng_build",
    "lib": {
      "entryFile": "src/public_api.ts",
      "umdModuleIds": {
        "@ngrx/store": "ngrx.store",
        "@ngrx/effects": "ngrx.effects",
        "@ngrx/entity": "ngrx.entity",
        "rxjs": "rxjs",
        "tslib": "tslib"
      }
    }
  }
}
